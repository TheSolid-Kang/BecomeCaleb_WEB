@model IEnumerable<BecomeCaleb_WEB.Models.TwoMitesTbl.dboSchema._TFBible>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Testament)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Chapter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Verse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descript)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Testament)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Chapter)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Verse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descript)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BibleSeq">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BibleSeq">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BibleSeq">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
